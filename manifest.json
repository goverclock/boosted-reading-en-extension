{
    "name": "Boosted Reading EN",
    "description": "Read English content faster.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}
